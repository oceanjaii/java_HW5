package test_p21;

public class Sample5_6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try
		{
			int[] test ;
			test = new int[5];
			
			System.out.print("將值指定給test[10]");
			
			test[10] = 80;
			
			System.out.print("將80指定給test[10]"); }
		
		catch(ArrayIndexOutOfBoundsException e) {
			System.out.print("超過陣列的範圍了");
		}
		
		System.out.print("順利地執行完畢");
			
	}

}
